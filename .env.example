# Copy the Template: Duplicate the .env.example file and rename it to .env.

# Application Environment
NODE_ENV=development # Required

# Server Configuration
PORT=3000 # Required

# Database Configuration
DB_HOST=localhost # Required
DB_USER=root # Required
DB_PASSWORD=yourpassword # Required
DB_NAME=yourdatabase # Required

# Telegram API Keys
API_ID=your_api_id # Required, Telegram Developer Portal
API_HASH=your_api_hash # Required, Telegram Developer Portal
STRING_SESSION=your_string_session # Optional, generated by the app

# Other Configurations
SECRET_KEY='your_secret_key' # Required, used for JWT, can be any random string
